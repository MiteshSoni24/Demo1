 SHOW DATABASES;

 DROP DATABASE DEMO1;

 USE DEMO;

 SHOW TABLES;

 DESC EMPLOYEE;

 DESC EMP_ADDRESS

 SELECT E.NAME,A.CITY FROM EMPLOYEE E,EMP_ADDRESS A WHERE E.ADDRESSINFO=A.ADDRESSID;

 SELECT E.ID,E.NAME,A.CITY,A.STATE,A.PIN FROM EMPLOYEE E,EMP_ADDRESS A WHERE E.ADDRESSINFO=A.ADDRESSID;

 DROP TABLE EMPLOYEE;

 SHOW TABLES;

 DELETE FROM EMP_ADDRESS WHERE ADDRESSID=1;

 ROLLBACK;

 SELECT * FROM EMP_ADDRESS;

